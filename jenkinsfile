pipeline{
    agent any
    stages{
        stage("used echo"){
            steps{
                echo 'hello akash tomer '
            }
            
        }
        stage("Rub a commands"){
            steps{
                sh '''pwd
                ls
                date
               '''
            }
            
        }
        stage("sleep command"){
            steps{
                sleep 10
            }
            
        }
        stage("creat a timestamp"){
            steps{
                timestamps {
                // some block
            }
        }
            
        }
        stage('Continue ?') {
            steps {
                input message: "Should we continue?", ok: "Yes"
            }
       }       

            
        }
    }

